{
  "name": "papbotts",
  "version": "2.0.0",
  "description": "PAPbot",
  "main": "src/index.ts",
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node --experimental-json-modules --es-module-specifier-resolution=node dist/src/index.js",
    "launch": "npm run build && npm run timezone && npm run start",
    "dev": "set NODE_ENV=development&& npm run launch",
    "prod": "set NODE_ENV=production1&& npm run launch",
    "production": "set NODE_ENV=production&& npm run launch",
    "timezone": "set TZ='Europe/Athens'",
    "test": "jest --passWithNoTests",
    "dev-test": "tsc -p tsconfig.test.json && npm test",
    "dev-update-commands": "set NODE_ENV='development'&& npm run build && npm run timezone && node --experimental-json-modules --es-module-specifier-resolution=node dist/src/Scripts/Commands/update.js",
    "prod-update-commands": "set NODE_ENV='production' && node dist/src/Scripts/Commands/update.js"
  },
  "author": "Papageorgiadis Savvas",
  "license": "ISC",
  "dependencies": {
    "discord-api-types": "^0.37.3",
    "discord.js": "^14.2.0",
    "find-config": "^1.0.0",
    "firebase": "^9.6.8",
    "firebase-admin": "^11.0.0",
    "googleapis": "^107.0.0",
    "greek-utils": "^1.3.0",
    "knex": "^2.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "node": "^18.7.0",
    "npm": "^8.8.0",
    "pg": "^8.7.3",
    "profanity-js": "^0.1.2",
    "readline": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rss-feed-emitter": "^3.2.3",
    "url-regex": "^5.0.0"
  },
  "devDependencies": {
    "@types/find-config": "^1.0.1",
    "@types/greek-utils": "^1.2.0",
    "@types/jest": "^28.1.1",
    "@types/lodash": "^4.14.179",
    "@types/pg": "^8.6.5",
    "dotenv": "^16.0.0",
    "jest": "^28.0.1",
    "typescript": "^4.7.4"
  }
}